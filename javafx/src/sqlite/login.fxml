<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sqlite.LoginController">
   <children>
      <Label fx:id="isConnected" layoutX="10.0" layoutY="50.0" prefHeight="49.0" prefWidth="475.0" text="Status" textFill="RED">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="76.0" layoutY="125.0" promptText="User Name">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <TextField fx:id="password" layoutX="76.0" layoutY="177.0" promptText="Password">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Button layoutX="124.0" layoutY="220.0" mnemonicParsing="false" onAction="#Login" text="Login">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
