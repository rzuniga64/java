<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="500.0"
            xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="linechart.Controller">
   <children>
      <LineChart fx:id="linechart" title="Line Chart">
        <xAxis>
          <CategoryAxis label="My X Label" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis label="My Y Label" side="LEFT" />
        </yAxis>
      </LineChart>
      <Button layoutX="224.0" layoutY="463.0" mnemonicParsing="false" onAction="#btn" text="Load Chart" />
   </children>
</AnchorPane>
