<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.media.MediaView?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="mediaplayer.Controller">
   <center>
      <MediaView fx:id="mediaview" fitHeight="200.0" fitWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
   <bottom>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btn1" mnemonicParsing="false" text="Play" />
            <Button fx:id="btn2" mnemonicParsing="false" text="Pause" />
            <Button fx:id="btn3" mnemonicParsing="false" text="Fast" />
            <Button fx:id="btn4" mnemonicParsing="false" text="Slow" />
            <Button fx:id="btn5" mnemonicParsing="false" text="Reload" />
            <Button fx:id="btn6" mnemonicParsing="false" text="Start" />
            <Button fx:id="btn7" mnemonicParsing="false" text="Last" />
            <Slider fx:id="volumeSlider">
               <HBox.margin>
                  <Insets left="80.0" />
               </HBox.margin>
            </Slider>
         </children>
      </HBox>
   </bottom>
</BorderPane>
